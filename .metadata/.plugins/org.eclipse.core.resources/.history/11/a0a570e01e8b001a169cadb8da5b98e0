package Fundamentos;

public class Wrapper {

	public static void main(String[] args) {

		//byte
		Byte b = 100;
		Short s = 1000;
		//Integer i = 10000; //int
		Integer i = Integer.parseInt("1000");
		Long l = 100000L;
		
		System.out.println(b.byteValue());
		System.out.println(s.toString());
		System.out.println(i);
		
	}
	
}
